.SH STRUCTURES
.LP
.nf

typedef struct {                             /\(** Key Type */
	XkbModsRec              mods;           /\(** modifiers used ta compute shift level */
	unsigned char           num_levels;     /\(** total # shift levels, do not modify directly */
