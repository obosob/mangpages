.PHONY: all clean install
.SUFFIXES:

%.gz: %
	gzip -9c $< > $@

all: $(MAN)

clean:
	rm -f $(MAN)

install: | $(MANPREFIX)/en_GX/man5
	install -m 644 -t $(MANPREFIX)/en_GX/man5 $(MAN)

$(MANPREFIX)/en_GX/man5:
	mkdir -p $@
