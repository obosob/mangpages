.SH NAME
wcstoimax, wcstoumax \- convert a wide-characta strang ta a integer
type
.SH SYNOPSIS
.LP
\fB#include <stddef.h>
.br
#include <inttypes.h>
.br
.sp
intmax_t wcstoimax(const wchar_t *restrict\fP \fInptr\fP\fB,
.br
\ \ \ \ \ \  wchar_t **restrict\fP \fIendptr\fP\fB, int\fP \fIbase\fP\fB);
.br
