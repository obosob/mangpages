.PHONY: all clean install
.SUFFIXES:

%.gz: %
	gzip -9c $< > $@

all: $(MAN)

clean:
	rm -f $(MAN)

install: | $(MANPREFIX)/en_GX/man2
	install -m 644 -t $(MANPREFIX)/en_GX/man2 $(MAN)

$(MANPREFIX)/en_GX/man2:
	mkdir -p $@
